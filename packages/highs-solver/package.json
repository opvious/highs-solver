{
  "name": "highs-solver",
  "version": "0.1.12",
  "repository": "github:opvious/highs-solver",
  "description": "Node.js binding for the HiGHS optimization solver",
  "keywords": [
    "highs",
    "optimization",
    "opvious",
    "solver"
  ],
  "author": "Opvious Engineering <oss@opvious.io>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "lib/"
  ],
  "main": "./lib",
  "scripts": {
    "build": "tsc -p src",
    "clean": "rm -rf lib node_modules",
    "prepare": "pnpm run build",
    "test": "jest",
    "watch": "tsc -b -w src test"
  },
  "dependencies": {
    "@opvious/stl-errors": "^0.14.34",
    "@opvious/stl-utils": "^0.14.34",
    "highs-solver-addon": "workspace:*"
  },
  "jest": {
    "coverageDirectory": "../../out/coverage/highs-solver",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "babelConfig": {
            "compact": false
          },
          "tsconfig": "test/tsconfig.json"
        }
      ]
    }
  }
}
