{
  "name": "highs-addon",
  "version": "0.4.1",
  "repository": "github:opvious/highs.ts",
  "description": "Low-level Node.js binding for the HiGHS optimization solver",
  "keywords": [
    "highs",
    "optimization",
    "opvious",
    "solver"
  ],
  "author": "Opvious Engineering <oss@opvious.io>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "prebuilds/",
    "src/",
    "binding-options.js",
    "CMakeLists.txt",
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "build": "cmake-js build",
    "clean": "rm -rf build node_modules",
    "postinstall": "pkg-prebuilds-verify ./binding-options.js || cmake-js build",
    "rebuild": "cmake-js rebuild",
    "test": "jest"
  },
  "dependencies": {
    "cmake-js": "^7.1.1",
    "node-addon-api": "^5.1.0",
    "pkg-prebuilds": "^0.1.0"
  },
  "devDependencies": {
    "tmp-promise": "^3.0.3"
  },
  "cmake-js": {
    "runtime": "node"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "jest": {
    "coverageDirectory": "../../out/coverage/highs-addon",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      ".d.ts",
      ".js"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "babelConfig": {
            "compact": false
          },
          "tsconfig": "test/tsconfig.json"
        }
      ]
    }
  }
}
